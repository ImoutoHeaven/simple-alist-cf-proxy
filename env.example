# Controller-driven worker (infra env only; strategies from controller config.yaml)
ENV=staging
ROLE=download
INSTANCE_ID=download-dev-1
APP_NAME=simple-alist-cf-proxy
APP_VERSION=dev

# Auth & integrity
TOKEN=your-hmac-token-here
WORKER_ADDRESS=https://your-worker.workers.dev
SIGN_SECRET=
SIGN_CHECK=true
HASH_CHECK=true
WORKER_CHECK=true
ADDITION_CHECK=true
ADDITION_EXPIRETIME_CHECK=true
IPV4_ONLY=true

# Controller access
CONTROLLER_URL=https://controller.example.com
CONTROLLER_API_PREFIX=/api/v0
CONTROLLER_API_TOKEN=replace-with-controller-token

# Cache/DO wiring (DO/KV names must match wrangler bindings)
BOOTSTRAP_CACHE_MODE=do
DECISION_CACHE_MODE=do
METRICS_MODE=do
BOOTSTRAP_DO=BootstrapDO
DECISION_DO=DecisionDO
METRICS_DO=MetricsDO
BOOTSTRAP_KV=BOOTSTRAP_KV
INTERNAL_API_TOKEN=replace-with-internal-token

# Optional: Cloudflare native rate limiter binding
ENABLE_CF_RATELIMITER=false
CF_RATELIMITER_BINDING=CF_RATE_LIMITER

# 策略类配置（地址/verifyHeader/PostgREST/路径策略/Throttle/FairQueue 等）均由 controller 下发，不应在 env 中配置。
